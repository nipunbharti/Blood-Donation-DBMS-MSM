<% include partials/donor%>

<div class="donorDetails">
	<table style="width: 100%" id="t01">
		<tr>
			<th>Name</th>
			<th>Age</th>
			<th>Sex</th>
			<th>Blood Group</th>
			<th>CId</th>
			<th>Amount</th>
		</tr>
		<% for(var i=0;i<result.length;i++){ %>
			<tr>
				<td><%= result[i].Name%></td>
				<td><%= result[i].Age%></td>
				<td><%= result[i].Sex%></td>
				<td><%= result[i].Blood_Group%></td>
				<td><%= result[i].CId%></td>
				<td><%= result[i].Amount%></td>
			</tr>
		<% } %>
	</table>
</div>

<% include partials/footer%>